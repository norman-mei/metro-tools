{
  "name": "metro-tools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:core",
    "dev:core": "node scripts/sync-tool-info-json.js && node scripts/sync-city-card-images.js && next dev --turbo -H 0.0.0.0",
    "dev:turbo": "node scripts/sync-tool-info-json.js && node scripts/sync-city-card-images.js && next dev --turbo",
    "build": "node scripts/sync-tool-info-json.js && node scripts/sync-city-image-folders.js && node scripts/sync-city-icons.js && node scripts/sync-city-card-images.js && node scripts/export-city-data.js && node scripts/generate-offline-manifest.js && prisma generate && next typegen && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check:duplicate-keys": "node scripts/check-duplicate-object-keys.js",
    "generate:station-totals": "node scripts/generate-station-totals.js",
    "sync:icons": "node scripts/sync-city-image-folders.js && node scripts/sync-city-icons.js",
    "sync:images": "node scripts/sync-city-image-folders.js",
    "generate:city-data": "node scripts/export-city-data.js",
    "generate:available-city-data": "node scripts/generate-available-city-data.js",
    "sync:icons:watch": "node scripts/auto-sync-city-icons.js",
    "sync:images:watch": "node scripts/auto-sync-city-image-folders.js",
    "sync:tool-info": "node scripts/sync-tool-info-json.js"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/cli": "^7.4.5",
    "@capacitor/core": "^8.0.2",
