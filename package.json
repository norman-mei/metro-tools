    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "color": "^4.2.3",
    "fast-glob": "^3.2.11",
    "feed": "^4.2.2",
    "framer-motion": "^12.34.0",
    "fuse.js": "^7.0.0",
    "glob": "^13.0.0",
    "i18next": "^25.8.7",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
